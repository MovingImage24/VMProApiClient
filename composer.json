{
    "name": "movingimage/vmpro-api-client",
    "description": "API client library for the VMPro public API",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Movingimage",
            "email": "developer@movingimage.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/log": "^1.0",
        "monolog/monolog": "^1.0",
        "namshi/jose": "^6.0 || ^7.0",
        "jms/serializer": "^3.0",
        "guzzlehttp/guzzle": "^6.0",
        "movingimage/video-manager-meta": "^3.0",
        "psr/cache": "^1.0",
        "cache/void-adapter": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9",
        "phpunit/phpunit": "^8.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "MovingImage\\Client\\VMPro\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MovingImage\\": "tests/MovingImage/",
            "MovingImage\\VMPro\\TestUtil\\": "tests/MovingImage/Util"
        }
    }
}
