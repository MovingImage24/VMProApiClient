{
  "name": "movingimage/vmpro-api-client",
  "description": "API client library for the VMPro public API",
  "type": "library",
  "license": "BSD 3-Clause License",
  "authors": [
    {
      "name": "Ruben Knol",
      "email": "ruben.knol@movingimage.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "doctrine/collections": "~1.3",
    "psr/log": "^1.0",
    "monolog/monolog": "^1.0",
    "namshi/jose": "^6.0 || ^7.0",
    "jms/serializer": "^1.0",
    "guzzlehttp/guzzle": "^5.0 || ^6.0",
    "movingimage/video-manager-meta": "dev-master",
    "psr/cache": "^1.0",
    "cache/void-adapter": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^1.12",
    "phpunit/phpunit": "^4.5 || ^5.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MovingImage\\Client\\VMPro\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MovingImage\\Client\\VMPro\\": "lib",
      "MovingImage\\Test\\": "tests/MovingImage/Test",
      "MovingImage\\TestCase\\": "tests/MovingImage/TestCase",
      "MovingImage\\VMPro\\TestUtil\\": "tests/MovingImage/Util"
    }
  }
}
